{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "rules": {
      "eol-last": ["error", "never"],
      "linebreak-style": 0,
      "no-unused-vars": ["error", { "vars": "all", "args": "after-used" }]
    
    }
}

